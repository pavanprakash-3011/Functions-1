#include <stdio.h>
void primenumber(int b){
    int i,f=0;
    for(i=2;i<=b/2;i++){
        if(b%i==0){
            f=1;
            break;
        }
    }
    if(f==0){
        printf("prime number");
    }
    else{
        printf("not a prime number");
    }
}
int main() {
    int a;
    scanf("%d",&a);
    primenumber(a);
    return 0;
}
